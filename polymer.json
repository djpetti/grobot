{
  "entrypoint": "index.html",
  "shell": "/src/grobot-app.html",
  "fragments": [
    "/src/main-view.html",
    "/src/my-view404.html"
  ],
  "sources": [
   "src/**/*",
   "images/**/*",
   "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/!(*test*)/*.{js,css,html}",
    "bower_components/!(*test*)/!(test|demo)/*.{js,css,html}",
    "bower_components/!(*test*)/!(test|demo)/**/*.{js,css,html}"
  ]
}
